{
  "manifest_version": 2,
  "name": "vaxtify",
  "description": "Distraction blocker for the stubborn",
  "version": "0.2.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "vaxtify@unneon.solar",
      "strict_min_version": "54.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "nativeMessaging",
    "tabs"
  ]
}
